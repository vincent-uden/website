<script>
const postBody = {
  path: new URL(document.URL).pathname,
};

document.addEventListener("astro:after-swap", async () => {
  await fetch("/analytics", {
    method: "POST",
    headers: {
      "Content-Type": "application/json",
    },
    body: JSON.stringify(postBody),
  });
});


</script>
