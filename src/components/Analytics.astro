---
const url = Astro.url.toString();
---

<script define:vars={{ url }}>
console.log(url);
const postBody = {
  path: new URL(url).pathname,
};

console.log(postBody);

fetch("/analytics", {
  method: "POST",
  headers: {
    "Content-Type": "application/json",
  },
  body: JSON.stringify(postBody),
});

</script>
